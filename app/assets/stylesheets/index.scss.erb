body {
  background: #ffffff;
}
// --------------------------------------------------------
.propocontainer {
  padding-top: 80px;
}
.toppage {
  margin-top: 90px;
}
.intro-header {
  position: relative;

  text-align: center;
  width: 100%;
  min-height: 100vh;
  color: #ffffff;
overflow: hidden;
}
.blur{
  /* If you're making other pages, make sure there is 50px of padding to make sure the navbar doesn't overlap content! */
  background: asset-url('parking.jpg') no-repeat center center;
  background-size: cover;
  -webkit-filter: blur(3px) grayscale(90%);
-moz-filter: blur(3px);
-o-filter: blur(3px);
-ms-filter: blur(3px);
filter: blur(3px) grayscale(100%);
position:absolute;
top: -2%;
left: -2%;
width:104%;
height:104%;
}
.filterblue{
  position:absolute;
  width:100%;
  height:100%;
  background-color:  rgba(0, 0, 0, 0.88)
}
.intro-message {
  position: relative;
  padding-top: 15%;
  padding-bottom: 8%;
}
.aroundblocbtn{
  margin-bottom: 20px
}
.aroundblocbtn .fa{
  float: none;
}
.intro-message .separator {
padding-top: 20px;
padding-bottom: 0;
}
.intro-message > h1 {
  margin: 10px;
  text-shadow: 2px 2px 3px rgba(0,0,0,0.6);
  margin-bottom: 50px
  // font-size: 5em;
}
.intro-message > h3 {
  margin-bottom: 20px
  // font-size: 5em;
}

.searchgroup {
  width: 100%;
}
.searchbar {
  min-width: 50% !important;
}


.loggedannounce {
  border: 2px solid rgb(163, 163, 163);
  border-radius: 5px;
  color: black;
  font-weight: 600;
  margin-top: 4%;
}


.intro-divider {
  width: 400px;
  border-top: 1px solid #f8f8f8;
  border-bottom: 1px solid rgba(0,0,0,0.2);
}
.intro-message > h3 {
  text-shadow: 2px 2px 3px rgba(0,0,0,0.6);
}
.reservation {
  font-size: 130%;
}
.enseignesdarty {
  height: 60px;
  width: 60px;
}
.enseignesauchan {
  height: 40px;
  width: 150px;
}
.enseignes {
  margin: 0 auto;
}
// -----------------------------------------NAVBAR
.navbar-brand {
  font-size: 24px;
}
.navbar-container {
  padding: 20px 0;
}
.navbar.navbar-fixed-top.fixed-theme {
  background-color: #FFF;
  border-color: #080808;
  box-shadow: 0 0 5px rgba(0,0,0,.8);
}
.navbar-brand.fixed-theme {
  font-size: 18px;
}
.navbar-container.fixed-theme {
  padding: 0;
}
.navbar-brand,
.navbar-brand.fixed-theme,
.navbar-container,
.navbar-container.fixed-theme,
.navbar.navbar-fixed-top.fixed-theme {
  transition: 0.8s;
  -webkit-transition: 0.8s;
}
.logo_hot {
  height: 60px;
  width: auto;
}
.link_navbar {
  padding-top: 20px;
}

.searchtitle {
  padding-top: 70px;
}
// -------------- reservationmodal
body.modal-open #wrap {
  -webkit-filter: blur(7px);
  -moz-filter: blur(15px);
  -o-filter: blur(15px);
  -ms-filter: blur(15px);
  filter: blur(15px);
  border: solid 1px;
}
.modal-backdrop {
  background: #f7f7f7;
}
.close {
  font-size: 50px;
  display: block;
}
.container,
.container-fluid {
  padding-top: 1%;
  padding-bottom: 1%;
}

.btn-search {
  padding: 8px 10px;
}
.textholderempty {
  font-size: 1.3em;
  color: rgb(186, 186, 186);
  padding-top: 50px;
  text-align: center;
}
.topformular {
  padding-top: 30px;
}
// -----------------------------------------------------edit user

.edit {
  padding-top: 50px;
}
// ------vertical ligne
div.vertical-line {
  width: 2px;
  /* Line width */
  background-color: red;
  /* Line color */
  height: 100%;
  /* Override in-line if you want specific height. */
  float: left;
  /* Causes the line to float to left of content.
    You can instead use position:absolute or display:inline-block
    if this fits better with your design */
}
.fa-dot-circle-o {
  color: red;
}


.postbiditem{
  padding-top: 2%
}

.homefooter{
  position: absolute;
  bottom: 0;
  width: 100%;
  height: auto;
  background: none
}

.homefooter ul li a,.homefooter ul li {
  color: rgb(125, 125, 125);
  text-decoration: none;
}
.homefooter ul li a:hover {
  color: rgb(255, 255, 255);
  text-decoration: none;
}


@media all and (max-width:991px) {
  .separator hr{
    display:block;
    width: 100%
  }
  .homefooter ul {
    display: inline;;
  }
}
